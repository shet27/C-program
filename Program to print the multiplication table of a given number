//multiplication table//
#include <stdio.h>
int main() 
{
   int num, i;
   clrscr();
   printf("Please enter any number to find multiplication table:");
   scanf("%d", &num);
   printf("Multiplication table for the given number %d: ", number);
   printf("         ");
   for(i=1;i<=10;i++){
      printf("%d x %d = %d", num,i,num* i);
      printf("            ");
   }
   getch();
   return 0;
}
